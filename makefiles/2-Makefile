CC = gcc
SRC := main.c school.c
OBJ := $(patsubst .c,.o,$(SRC))
NAME = school

all: $(NAME) $(OBJ)

$(NAME): $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

$(OBJ): $(SRC)
	$(CC) -c -o $(OBJ) $(SRC)

#main.o: main.c
#	$(CC) -c -o main.o main.c

#school.o: school.c
#	$(CC) -c -o school.o school.c

clean:
	rm -f $(NAME) main.o school.o
